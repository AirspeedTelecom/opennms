  <artifactId>jasper-dependencies</artifactId>
  <packaging>jar</packaging>
  <name>OpenNMS Jasper Report Dependencies</name>
  <dependencies>
    <dependency>
      <groupId>jasperreports</groupId>
      <artifactId>jasperreports</artifactId>
      <version>3.6.0</version>
      <exclusions>
        <exclusion>
